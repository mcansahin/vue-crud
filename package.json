{
  "name": "vue-crud",
  "version": "0.1.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib -name vue-crud ./src/build.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bulma": "^0.7.5",
    "core-js": "^2.6.10",
    "material-design-icons": "^3.0.1",
    "noty": "^3.2.0-beta",
    "vue": "^2.6.10",
    "vue-flatpickr-component": "^8.1.5",
    "vue-js-toggle-button": "^1.3.3",
    "vue-location-picker": "^1.0.1",
    "vue-range-component": "^1.0.3",
    "vue-select": "^3.2.0",
    "vue-trix": "^0.9.0",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "main": "./dist/vue-crud.common.js",
  "repository": {
    "type": "git",
    "url": "git://https://github.com/mcansahin/vue-crud.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/mcansahin/"
  },
  "license": "GNU General Public License (GPL)"
}
